# OpenRouter API Keys (comma-separated)
# Get your keys from https://openrouter.ai/keys
OPENROUTER_API_KEYS=your-openrouter-api-key-here

# Server configuration
PORT=3000

# Basic Authentication (optional)
AUTH_ENABLED=false
AUTH_USERNAME=admin
AUTH_PASSWORD=password

# API Key Authentication (optional)
API_KEY_AUTH_ENABLED=true

# Client API Keys (format: name:key:rateLimit, comma-separated)
# This will be populated automatically when you generate keys
CLIENT_API_KEYS=

# OpenRouter API URL
OPENROUTER_API_URL=https://openrouter.ai/api/v1
